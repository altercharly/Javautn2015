package ejercicio2;

import java.util.Scanner;
public class Principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		private int primero;
		vnm = new Vectnummay();
		Scanner en = new Scanner(System.in);
		
		primero = en.nextInt(); 
		vnm.cargar(primero);
		vnm.imprimir();
		System.out.println("el valor ingresado es:" + primero);
	}

}
